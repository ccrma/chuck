// instantiate a PRC
PRC tokens;
string out;

// set the tokens
tokens.set( "a b c 3.343 2.3" );

// get size
<<< "number of tokens:", tokens.size() >>>;

// print out
tokens.get( 0, out ); <<< out >>>;
tokens.get( 1, out ); <<< out >>>;
tokens.get( 2, out ); <<< out >>>;
tokens.get( 3, out ); <<< out >>>;
tokens.get( 4, out ); <<< out >>>;

